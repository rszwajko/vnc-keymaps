{
  "name": "vnc-keymaps",
  "version": "0.0.1",
  "type": "module",
  "description": "Keymps for VNC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com:kubevirt-ui/vnc-keymaps.git"
  },
  "author": "Radoslaw Szwajkowski <rszwajko@redhat.com>",
  "license": "Apache-2.0",
  "keywords": [
    "QEMUExtendedKeyEvent",
    "VNC",
    "Qemu",
    "scancode",
    "keysym",
    "keystroke"
  ],
  "engines": {
    "yarn": "^1.22.0 ",
    "node": ">=20.19.0"
  },
  "scripts": {
    "prebuild": "yarn test",
    "build": "tsup",
    "pretest": "yarn lint",
    "test": "jest --rootDir=. --config=jest.config.json",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "yarn lint --fix "
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "minify": false,
    "dts": true
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}