{
  "name": "@kubevirt-ui/vnc-keymaps",
  "version": "0.0.1",
  "type": "module",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "Keymps for VNC",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "./dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kubevirt-ui/vnc-keymaps.git"
  },
  "author": "Radoslaw Szwajkowski <rszwajko@redhat.com>",
  "license": "MIT",
  "keywords": [
    "QEMUExtendedKeyEvent",
    "VNC",
    "Qemu",
    "scancode",
    "keysym",
    "keystroke"
  ],
  "packageManager": "npm@10.9.4",
  "engines": {
    "npm": "^10.9.4",
    "node": "^22.21.1"
  },
  "scripts": {
    "build": "npm run test && tsup",
    "test": "npm run lint && jest --rootDir=. --config=jest.config.json",
    "lint": "eslint . && prettier . --check",
    "lint:fix": "eslint . --fix && prettier . --write"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": [
      "esm",
      "cjs"
    ],
    "minify": false,
    "dts": true
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.28.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.3.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
